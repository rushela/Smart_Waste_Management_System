          "name": "Create Collection - Not Collected",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"binID\": \"BIN-R003\",\n  \"wasteType\": \"recyclable\",\n  \"weight\": 0,\n  \"notes\": \"Bin blocked by parked vehicle\",\n  \"workerId\": \"WORKER-001\",\n  \"status\": \"not_collected\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/collections",
              "host": ["{{baseUrl}}"],
              "path": ["api", "collections"]
            }
          }
        },
