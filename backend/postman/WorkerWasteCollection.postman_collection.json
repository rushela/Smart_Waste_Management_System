          "name": "Update Collection Status",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"status\": \"partial\",\n  \"notes\": \"Corrected - was overflow\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/collections/{{collectionId}}",
              "host": ["{{baseUrl}}"],
              "path": ["api", "collections", "{{collectionId}}"]
            }
          }
        },
