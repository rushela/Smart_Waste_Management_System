            "body": {
              "mode": "raw",
              "raw": "{\n  \"weight\": 6.8,\n  \"notes\": \"Weight corrected after re-weighing\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/collections/{{collectionId}}",
              "host": ["{{baseUrl}}"],
              "path": ["api", "collections", "{{collectionId}}"]
            }
          }
        },
        {
          "name": "Delete Collection",
          "request": {
            "method": "DELETE",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/api/collections/{{collectionId}}",
              "host": ["{{baseUrl}}"],
              "path": ["api", "collections", "{{collectionId}}"]
            }
          }
        }
      ]
    }
  ],
